ily */\r\n  --ff-base: 'Inter', sans-serif;\r\n\r\n  /*  font sizes */\r\n  --fs-300: 0.9rem;\r\n  --fs-320: 1rem;\r\n  --fs-350: 1.2rem;\r\n  --fs-400: 1.3rem;\r\n  --fs-450: 1.5rem;\r\n  --fs-500: 1.8rem;\r\n  --fs-600: 2.4rem;\r\n  --fs-650: 2.8rem;\r\n  --fs-700: 3.1rem;\r\n\r\n  /* mobile screen */\r\n  --fs-base: var(--fs-300);\r\n  --fs-hero-heading: var(--fs-600);\r\n  --fs-heading: var(--fs-500);\r\n  --fs-sub-heading: var(--fs-400);\r\n  --fs-sub-title: var(--fs-300);\r\n  --fs-btn: var(--fs-300);\r\n\r\n  /* font weight */\r\n  --fw-normal: 500;\r\n  /* --fw-medium: 500; */\r\n  --fw-semibold: 600;\r\n  --fw-bold: 700;\r\n  --fw-extraBold: 900;\r\n\r\n  --screen-Max-Width: 1220px;\r\n  --sections-spacing: 6rem;\r\n  --sections-padding: 1.2rem;\r\n\r\n  /* 2 / 4 / 8 / 12 / 16 / 24 / 32 / 48 / 64 / 80 / 96 / 128\r\n*/\r\n  --spacing-s: 8px;\r\n  --spacing-m: 12px;\r\n  --spacing-l: 16px;\r\n  --spacing-l1: 24px;\r\n  --spacing-l2: 32px;\r\n  --spacing-l3: 48px;\r\n  --spacing-l4: 64px;\r\n  --spacing-l5: 96px;\r\n  --spacing-l6: 128px;\r\n\r\n  /* border raduse */\r\n  --border-r: 0.3125rem;\r\n\r\n  --text-max-width: min(80%, 30em);\r\n\r\n  /* --shadow */\r\n  --shadow: 0px 4px 9.9px 0px rgba(120, 129, 144, 0.15);\r\n  --shadow-lt: 0px 0px 13px -6px rgba(0, 0, 0, 0.51);\r\n\r\n  /* transitions */\r\n  --base-transition: all 280ms ease;\r\n\r\n  --phone-breakpoint: 50em;\r\n  --tap-breakpoint: 70em;\r\n}\r\n\r\n/* global classes */\r\n.container {\r\n  padding: var(--sections-padding);\r\n  width: min(var(--screen-Max-Width), 100%);\r\n  margin: 0 auto;\r\n}\r\n\r\n@media (min-width: 50em) {\r\n  :root {\r\n    --fs-base: var(--fs-400);\r\n    --fs-hero-heading: var(--fs-700);\r\n    --fs-heading: var(--fs-600);\r\n    --fs-sub-title: var(--fs-400);\r\n    --fs-btn: var(--fs-400);\r\n  }\r\n}\r\n\r\n/* ============================================ \r\n                  global css\r\n============================================  */\r\n/* html {\r\n  to make every 1rem = 10px \r\n   10px / 16px = 0.625 = 62.5% \r\n  font-size: 62.5%;\r\n} */\r\n/* @media (min-width: var(--phone-breakpoint)) {\r\n  html {\r\n    font-size: 70%;\r\n\r\n} */\r\n\r\nbody {\r\n  /* typography */\r\n  color: var(--clr-base);\r\n  font-family: 'Inter', sans-serif;\r\n  font-size: var(--fs-base);\r\n  font-weight: var(--fw-normal);\r\n  /* letter-spacing: 0.6px; */\r\n  line-height: 1.2;\r\n  text-rendering: optimizeSpeed;\r\n\r\n  /* body style */\r\n  background-color: var(--clr-base-lt);\r\n  min-height: 100vh;\r\n}\r\n\r\nsvg {\r\n  width: 2rem;\r\n  height: 2rem;\r\n  fill: var(--clr-base);\r\n}\r\n\r\n/* ============================================ \r\n                  global classes\r\n============================================  */\r\n.bk-clr-primarylt {\r\n  background-color: var(--clr-primary-lt);\r\n}\r\n.flex-row {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.flex-column {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: baseline;\r\n}\r\n.clr-base {\r\n  background-color: var(--clr-base);\r\n}\r\n\r\n.even-columns {\r\n  display: grid;\r\n  gap: 2rem;\r\n}\r\n.even-columns > * {\r\n  justify-self: start;\r\n  align-self: center;\r\n  overflow: hidden;\r\n}\r\n\r\n@media (min-width: 50em) {\r\n  .even-columns {\r\n    grid-auto-flow: column;\r\n    grid-auto-columns: 1fr;\r\n  }\r\n}\r\n\r\n.btn {\r\n  font-weight: var(--fw-semiBold);\r\n  font-size: var(--fs-btn);\r\n  color: var(--clr-base-lt);\r\n  background-color: var(--clr-primary);\r\n  border-radius: var(--border-r);\r\n  display: inline-block;\r\n  cursor: pointer;\r\n  padding: var(--spacing-s) var(--spacing-l);\r\n  margin: 1rem 0;\r\n  border: none;\r\n  transition: var(--base-transition);\r\n}\r\n.btn:hover {\r\n  transform: translateY(-1.5px);\r\n}\r\n/* for sections with smooth reveal */\r\n.hid-section {\r\n  transform: translateY(50%);\r\n  filter: blur(5px);\r\n  opacity: 0;\r\n}\r\n\r\n.link-hover:hover {\r\n  color: var(--clr-primary);\r\n  transition: color ease-in-out 200ms;\r\n}\r\n\r\n.icon {\r\n  width: 32px;\r\n  height: 32px;\r\n}\r\n.icon-24px {\r\n  width: 24px;\r\n  height: 24px;\r\n}\r\n.overlay-blur {\r\n  position: absolute;\r\n  top: 0;\r\n  right: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  z-index: 10;\r\n  backdrop-filter: blur(5px);\r\n}\r\n\r\n.hidden {\r\n  display: none;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d2fb0a16c4d9ad0e5103a03f25e0437d","cacheData":{"env":{}}}